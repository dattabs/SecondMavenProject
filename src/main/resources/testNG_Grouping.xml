<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Grouping in TestNG Suite">
	<groups>
		<run>
			<include name = "sanity"> </include>
			<include name = "regression"> </include>
		
		</run>
	</groups>
  <test thread-count="5" name="Functional Test">
    <classes>
      <class name="SecondMavenProject.SecondMavenProject.GroupingMethods"/>
      <class name="SecondMavenProject.SecondMavenProject.Annotations"/>
      <class name="SecondMavenProject.SecondMavenProject.DependsOnMethod"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->